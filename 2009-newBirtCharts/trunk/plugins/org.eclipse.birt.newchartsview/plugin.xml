<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>

   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            name="Perspective"
            class="org.eclipse.birt.newcharts.views.Perspective"
            id="org.eclipse.birt.newcharts.views.perspective">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            name="View"
            class="org.eclipse.birt.newcharts.views.NewChartsView"
            id="org.eclipse.birt.newcharts.views.newchartsview">
      </view>
   </extension>
   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="org.eclipse.birt.newcharts.views.Application">
         </run>
      </application>
   </extension>
   
</plugin>
